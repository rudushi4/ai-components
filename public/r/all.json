      "type": "registry:ui"
    },
    {
      "path": "components/chatbot/prompt-input.tsx",
      "type": "registry:ui"
    },
    {
      "path": "components/chatbot/code-block.tsx",
      "type": "registry:ui"
    },
    {
      "path": "components/chatbot/chain-of-thought.tsx",
      "type": "registry:ui"
    },
    {
      "path": "components/chatbot/reasoning.tsx",
      "type": "registry:ui"
    },
    {
